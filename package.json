{
  "name": "vue-auto-i18n",
  "version": "0.5.2",
  "license": "MIT",
  "homepage": "https://github.com/chetzof/vue-auto-i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/chetzof/vue-auto-i18n.git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.20.0",
    "flat": "^5.0.2"
  },
  "devDependencies": {
    "@types/flat": "^5.0.1",
    "@types/lodash": "^4.14.161",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "3.8.3",
    "vue": "^2.0.0",
    "vue-i18n": "^8.0.0"
  },
  "peerDependencies": {
    "vue": "^2.0.0",
    "vue-i18n": "^8.0.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc --build tsconfig.prod.esm.json && tsc --build tsconfig.prod.cjs.json",
    "watch": "npm-run-all --parallel watch-esm watch-cjs",
    "watch-esm": "tsc -w -p tsconfig.prod.esm.json",
    "watch-cjs": "tsc -w -p tsconfig.prod.cjs.json",
    "test": "jest --passWithNoTests",
    "test--watch": "yarn test --watch --runInBand",
    "format": "eslint --cache --ext .js,.jsx,.ts,.tsx,.json --fix ./ >/dev/null 2>&1 || true",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  ./"
  }
}
